<div class="container">
    <h1><%= user.name %></h1>
    <% if (user.admin) {%>
    <td><img src="/images/engine-red.png"></td>
    <% } else { %>
    <td><img src="/images/engine-gray.png"></td>
    <% } %>
    <hr>
    <h3><%= i18n('user').contact %> <%= user.email %></h3>

    <a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>"><%= i18n('user').edit %></a>
</div>